                $("#Capacity").empty();
                $.each(data, function (index, item) {
                    $("#Capacity").append("<option value='" + item.Value + "'>" + item.Text + "</option>");
                });
            });
        }
        else {
            //remove required attribute
            $("#PaddySize").attr("required", "required");
            $("#Type").attr("required", "required");
            $("#Pass").attr("required", "required");
            $("#Capacity").attr("required", "required");
