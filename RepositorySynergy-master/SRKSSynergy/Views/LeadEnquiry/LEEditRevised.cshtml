            $('#pindrp option[value=' + Pincode + ']').prop('selected', 'selected');
        }, 700);
        //alert(2)
    });
</script>

@*div hiding and displaying*@
<script type="text/Javascript">
    $(document).ready(function () {
        $("#yesorno").change(function () {
            var counter = document.getElementById("yesorno").value;
            var cmdtyp = $('#CommodityTypedrp option:selected').text();
            if (counter == 'Yes') {
                //$("#capacity").attr("style", "display:block;padding-left:60px");
                $("#timeframe").attr("style", "display:block;padding-left:60px");
                $("#typeofrequirement").attr("style", "display:block;padding-left:60px");
                $("#QuotesToBeSubmittedc").attr("style", "display:block");
                $("#EnquiryHandledByc").attr("style", "display:block");
                $("#machines").attr("style", "padding-left:60px");

                // $("#SorterModelc").attr("style", "display:block;");//code added by sneha 17th July 2017
                //$("#machines").attr("style", "display:block");

                //make these fields required
                // $("#capcty").attr("required", "required");
                $("#typeofreq").attr("required", "required");
                $("#TimeFrame").attr("required", "required");
                $("#EnquiryHandledBy").attr("required", "required");
                //alert(cmdtyp);
                if (cmdtyp == 'Pulses') {
                    $("#machines").hide();
                    $('#typeofreq').val('Sorters');
                    $('#typeofreq').attr("disabled", true);
                    $("#SorterModelc").attr("style", "display:block;");//code added by sneha 17th July 2017
                    $("#machines_gl").hide();
                    $("#machines_gm").hide();
                }
                else if (cmdtyp == 'Rice') {

                    if ($('#newmill').attr('checked')) {
                        $("#hideonnewmill").hide();
                        $("#typeofreq").attr("required", "required");
                        $("#typeofreq").attr("disabled", false);
                        $("#machines").hide();
                        $("#machines_gl").hide();
                        $("#machines_gm").hide();
                        $("#line2").hide();
                    }
                    else {
                        $("#hideonnewmill").show();
                        $("#typeofreq").attr("required", "required");
                        $("#typeofreq").attr("disabled", false);
                        $("#machines").hide();
                        $("#machines_gl").hide();
                        $("#machines_gm").hide();
                    }

                }
                else if (cmdtyp == 'Grain Milling') {
                    $("#typeofreq").removeAttr('required');
                    $("#typeofrequirement").hide();
                    $("#machines").hide();
                    $("#machines_gm").show();
                    $("#machines_gl").hide();
                }
                else if (cmdtyp == 'Grain Logistics') {
                    $("#typeofreq").removeAttr('required');
                    $("#typeofrequirement").hide();
                    $("#machines").hide();
                    $("#machines_gl").show();
                    $("#machines_gm").hide();
                }
                else {
                    $('#typeofreq').attr("disabled", false);
                    $('#typeofreq').val('--Select--');
                }
            }
            else {


                $("#timeframe").attr("style", "display:none");
                $("#typeofrequirement").attr("style", "display:none");
                $("#QuotesToBeSubmittedc").attr("style", "display:none");
                $("#EnquiryHandledByc").attr("style", "display:none");
                $("#machines").attr("style", "display:none");
                $("#machines_gl").hide();
                $("#machines_gm").hide();
                //make these fields required
                //$("#SorterModelc").removeAttr("required");
                $("#typeofreq").removeAttr("required");
                $("#SorterModelc").attr("style", "display:none;");
                $("#TimeFrame").removeAttr("required");
                $('#typeofreq').attr("disabled", false);
                $('#typeofreq').val('--Select--');

            }
        });

        $(document).ready(function () {
            $("#typeofreq").change(function () {
                var counter1 = document.getElementById("typeofreq").value;
                if (counter1 == 'Machines') {
                    $("#machines").attr("style", "display:block;padding-left:60px");
                    $("#SorterModelc").attr("style", "display:none;");

                }
                else if (counter1 == 'Sorters') {
                    $("#SorterModelc").attr("style", "display:block;padding-left:60px;");
                }
                else {
                    $("#machines").attr("style", "display:none");
                }
            });
        });



    });
</script>

