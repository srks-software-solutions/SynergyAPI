<script type="text/Javascript">
    $(document).ready(function () {
        var State = $("#State").val();
        var City = $("#City").val();
        var Pincode = $("#Pincode").val();

        $('#Stateid option[value=' + State + ']').prop('selected', 'selected');
        $('#Stateid').trigger("change");
        //alert(City)
        setTimeout(function () {

            $('#citydrp option[value=' + City + ']').prop('selected', 'selected');
