<script>
    //phone number with 10digits
    $('.keyup-limit-10').keyup(function () {
        $('span.error-keyup-3').remove();
        var inputVal = $(this).val();
        var characterReg = /^([a-zA-Z0-9]{10,10})$/;
        if (!characterReg.test(inputVal)) {
            $(this).after('<span class="error error-keyup-3" style="color:red" >Minimum 10 digits.</span>');
        }
    });

    //Pin code number with 6digits
    $('.keyup-limit-6').keyup(function () {
        $('span.error-keyup-3').remove();
        var inputVal = $(this).val();
        var characterReg = /^([a-zA-Z0-9]{6,6})$/;
        if (!characterReg.test(inputVal)) {
            $(this).after('<span class="error error-keyup-3" style="color:red" >Minimum 6 Digits.</span>');
        }
    });

    //Mill Name No special char / numbers/ . , / are not allowed
    $('.millname').keyup(function () {
        $('span.error-millname-3').remove();
        var inputVal = $(this).val();
        var characterReg = /^([a-zA-Z0-9'' ''.'.])+$/;
        //var characterReg = /^([a-zA-Z0-9]{6,6})$/;
        if (!characterReg.test(inputVal)) {
            $(this).after('<span class="error error-millname-3" style="color:red" >No Special and Numbers are not Allowed</span>');
        }
    });

    //tel no
    $('.keyup-limit-11').keyup(function () {
        $('span.error-keyup-3').remove();
        var inputVal = $(this).val();
        var characterReg = /^([a-zA-Z0-9]{1,11})$/;
        if (!characterReg.test(inputVal)) {
            $(this).after('<span class="error error-keyup-3" style="color:red" >Only Numbers and Maximum 11 digits.</span>');
        }
    });

    //income number with 7digits
    $('.keyup-limit-7').keyup(function () {
        $('span.error-keyup-3').remove();
        var inputVal = $(this).val();
        var characterReg = /^([a-zA-Z0-9]{1,7})$/;
        if (!characterReg.test(inputVal)) {
            $(this).after('<span class="error error-keyup-3" style="color:red" >Maximum 7 digits.</span>');
        }
    });

    //email validation
    $(".keyup-email").keyup(function () {
        $('span.error-keyup-7').remove();
        var inputVal = $(this).val();
        var emailReg = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;
        if (!emailReg.test(inputVal)) {
            $(this).after('<span class="error error-keyup-7">Invalid Email Format.</span>');
        }
    });

    //For Sub category
    $(".subcategory").keyup(function () {
        $('span.error-keyup-7').remove();
        var inputVal = $(this).val();
        var characterReg = /([A-Z0-9A-Z])$/;
        if (!characterReg.test(inputVal)) {
            $(this).after('<span class="error error-keyup-7" style="color:red">Invalid Category</span>');
        }
    });
</script>

