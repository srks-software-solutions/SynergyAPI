<script type="text/Javascript">
    $(document).ready(function () {
        //alert(9)
        $('#Stateid').change(function (e) {
            e.preventDefault();
            var url = '@Url.Action("GettingDistrict")';
            var masprd = $('#Stateid option:selected').text();
            $.post(url, { prdts: masprd }, function (data) {
                var items = [];
                items.push("<option>--Select District--</option>");
                $.each(data, function () {
                    items.push("<option value=" + this.Value + ">" + this.Text + "</option>");
                });
                $("#citydrp").html(items.join(' '));
            });
        });
    });
</script>

