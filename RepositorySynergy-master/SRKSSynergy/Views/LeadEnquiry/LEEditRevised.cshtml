<div style="background: gray; padding-bottom: 15px; width: 1500px">
    <p class="txtft">Edit RC Single Machine Enquiry</p>
</div>
<div class="container" style="width: 1500px; padding-top: 3px">
    <div style="background: #e5e5e5">
        @using (Html.BeginForm("LEEditRevised", "LeadEnquiry", FormMethod.Post))
        {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true)

            @Html.HiddenFor(m => m.LERID)
            @Html.HiddenFor(m => m.CreatedBy)
            @Html.HiddenFor(m => m.CreatedOn)
            @Html.HiddenFor(m => m.IsDeleted)
            @Html.HiddenFor(m => m.IsStatus)
            @Html.HiddenFor(m => m.CPID)
            @Html.HiddenFor(m => m.IsDrop)
            @Html.HiddenFor(m => m.LeadTime)
            @Html.HiddenFor(m => m.IsTime)
            @Html.HiddenFor(m => m.IsCount)
            @Html.HiddenFor(m => m.IsHOD)
            @Html.HiddenFor(m => m.Country)
            @Html.HiddenFor(m => m.NotifyDate)
            @Html.HiddenFor(m => m.Latitude)
            @Html.HiddenFor(m => m.Longitude)
            @Html.HiddenFor(m => m.State)
            @Html.HiddenFor(m => m.City)
            @Html.HiddenFor(m => m.Pincode)

            <div class="row" style="padding-top: 10px">

                @if (TempData["date"] != null)
                {
                    <p style="color: red; font-weight: 900; padding-left: 170px">@TempData["date"]</p>
                }

                @if (Session["WrongPincode"] != null)
                {
                    <div class="ErrorMsg">
                        <p style="color: red; font-size: 4vh; font-weight: 900; text-align: center">@Session["WrongPincode"]</p>
                    </div>
                    Session["WrongPincode"] = null;
                }
                <div>
                    <div style="padding-left: 40px">
                        <table class="table " style="width: 1500px; padding-left: 30px;">

                                <tr>
                                    <td class="span2">
                                        <div style="text-align: left; vertical-align: middle;">
                                            <label><strong>Name of Collector</strong></label>
                                        </div>
                                    </td>
                                    <td class="span3">
                                        <div>
                                            @Html.TextBoxFor(model => model.NameofCollector, new { id = "mfrto", @class = "fntst text1", @placeholder = "Name of Collector", @required = "required" })
                                        </div>
                                    </td>
                                    <td class="span2">
                                        <div style="text-align: left; vertical-align: middle;">
                                            <label><strong>Date</strong></label>
                                        </div>
                                    </td>
                                    <td class="span3">
                                        <div>
                                           @Html.TextBoxFor(model => model.LeadDate, new { @id = "LeadDate", @name = "LeadDate", @class = "fntst", @readonly = "readonly", Value = @ViewBag.Date })


                                        </div>
                                    </td>
                                </tr>

                                <tr>
                                    <td class="span2">
                                        <div style="text-align: left; vertical-align: middle;">
                                            <label><strong>Mill Name </strong></label>
                                        </div>
                                    </td>
                                    <td>
                                        <div>
                                            @Html.TextBoxFor(model => model.MillName, new { id = "mfrto", @class = "fntst millname text1", @placeholder = "Mill Name", @required = "required" })
                                        </div>
                                    </td>

                                    <td class="span2">
                                        <div style="text-align: left; vertical-align: middle;">
                                            <label><strong>Mill Type</strong></label>
                                        </div>
                                        <div class="controls">
                                            @{
                List<SelectListItem> listItems1 = new List<SelectListItem>();
                listItems1.Add(new SelectListItem
                {
                    Text = "Private Limited",
                    Value = "Private Limited"
                });
                listItems1.Add(new SelectListItem
                {
                    Text = "Government",
                    Value = "Government",
                });
                listItems1.Add(new SelectListItem
                {
                    Text = "Public Listed",
                    Value = "Public Listed"
                });
                listItems1.Add(new SelectListItem
                {
                    Text = "Sole Proprietorship",
                    Value = "Sole Proprietorship"
                });
                listItems1.Add(new SelectListItem
                {
                    Text = "Others",
                    Value = "others1"
                });
                                            }
                                        </div>
                                    </td>
                                    <td>
                                        <div>
                                            @Html.DropDownListFor(model => model.MillType, listItems1, "-- Select Type --", new { @id = "orgty", @name = "orype", @class = "fntst", @required = "required" })
                                            @*@Html.TextBoxFor(model => model.MillType, new { @class = "fntst", @style = "display: none", @id = "orype" , @required = "required" })*@
                                        </div>
                                    </td>
                                </tr>

                                <tr>
                                    <td class="span2">
                                        <div style="text-align: left; vertical-align: middle;">
                                            <label><strong>Address Line 1 </strong></label>
                                        </div>
                                    </td>
                                    <td class="span3">
                                        <div>
                                            @Html.TextBoxFor(model => model.AddressLine1, new { id = "mfrto", @class = "fntst", @placeholder = "Address Line 1", @required = "required" })
                                        </div>
                                    </td>
                                    <td class="span2">
                                        <div style="text-align: left; vertical-align: middle;">
                                            <label><strong>Address Line 2</strong></label>
                                        </div>
                                    </td>
                                    <td>
                                        <div>
                                            @Html.TextBoxFor(model => model.AddressLine2, new { id = "mfrto", @class = "fntst text1", @placeholder = "Landmark ", @required = "required" })
                                        </div>
                                    </td>
                                </tr>

                                <tr>
                                <td class="span2">
                                    <div style="text-align: left; vertical-align: middle;">
                                        <label><strong>State </strong></label>
                                    </div>
                                </td>
                                <td class="span3">
                                    <div>
                                          @Html.DropDownListFor(m => m.State, (SelectList)ViewData["State"], "--Select State--", new { id = "Stateid", @name = "Stateid", @required = "required" })
                                        @Html.CheckBox("addcitypinchk", new { id = "addcitypinchk", @name = "addcitypinchk", @class = "inline", @type = "checkbox" }) To Add City and PIN Code
                                    </div>
