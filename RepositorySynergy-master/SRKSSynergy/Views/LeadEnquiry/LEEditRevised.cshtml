<script>
    //For restricting any field to its maxlength size
    $(document).ready(function () {
        $('input').keyup(function (e) {
            if (this.value.length == $(this).attr("maxlength")) {
                e.preventDefault();
                $(event.target).next("span").html("You Cannot Enter More..").show().fadeOut(2000);
                return false;
            }
            return true;
        });
    });
</script>

