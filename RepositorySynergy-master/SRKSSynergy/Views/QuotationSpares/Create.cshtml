	@* Update Quotation Number from DB for every 100 milli seconds *@
	$(document).ready(function () {
		setInterval(updatequotnumber, 100);
		function updatequotnumber() {
			var mdbid = '@ViewBag.MDBID';
			if (mdbid != '') {
