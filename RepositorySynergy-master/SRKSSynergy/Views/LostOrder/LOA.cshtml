						<div class="controls-row">
							<tr>
								<td class="span10">
									<div style="text-align: left; vertical-align: middle;">
										<label>
											<strong>Additional Comments : </strong>
										</label>
									</div>
								</td>
							</tr>
							<tr>
								<td class="span10">
									@Html.TextAreaFor(m => m.comments, new { @id = "comm", @name = "comm", rows = 4, columns = 20, @style = "width:900px",@required="required", @placeholder = "Additional Comments" })
									@*<textarea rows="3" class="span10" type="text" maxlength="360" name="additionalcomments"></textarea>*@</td>
							</tr>
						</div>
					</table>
						</div>
					<div class="span10" style="padding-left:60px">
						<div class="span3 cent">
							@*<button class="btn btn-large btn-success" type="button">Generate</button>*@
