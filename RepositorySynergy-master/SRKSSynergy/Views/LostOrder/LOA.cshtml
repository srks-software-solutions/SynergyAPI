									<div style="text-align: left; vertical-align: middle;">
										<label><strong>Date :</strong></label></div>
								</td>
								<td class="span3">
									<div>
										@Html.TextBoxFor(model => model.LOADate, new { id = "Loadate", @class = "fntst", @placeholder = "Quotation Number", Value = @System.DateTime.Today.ToString("yyyy-MM-dd hh:mm:ss tt"), @readonly = "readonly", @style = "display:none;" })
