@using (Html.BeginForm("OACheck", "OA", new { mdbid = @ViewBag.MDBID }, FormMethod.Post))
{
	@Html.AntiForgeryToken()
	@Html.ValidationSummary(true)
	<div>
		<div style="background: gray; padding-bottom: 15px">
			<p class="txtft">Order Acknowledgement</p>
		</div>
		@*@Html.TextBoxFor(model => model.OAEquipGeneralData.OADate, new { id = "quotdate", @class = "fntst", @placeholder = "Quotation Number", Value = @System.DateTime.Today.ToString("yyyy-MM-dd hh:mm:ss tt"), @readonly = "readonly", @style = "display:none;" })*@
        @Html.HiddenFor(model => model.OAEquipGeneralData.OADate)
		@Html.HiddenFor(model => model.OAEquipGeneralData.OAID)
		@Html.HiddenFor(model => model.OAEquipGeneralData.MDBID)
		@Html.HiddenFor(model => model.OAEquipGeneralData.QGID)
		@Html.HiddenFor(model => model.OAEquipGeneralData.CPID)
		@Html.HiddenFor(model => model.OAEquipPayment.OAPID)
		@Html.HiddenFor(model => model.OAEquipPayment.OAID)

		<div class="container" style="width: 1000px; padding-top: 3px">
			<div style="background: #e5e5e5">
				<div style="float: right; padding-right: 15px">
					<label class="fntst1">&nbsp;&nbsp;&nbsp;&nbsp;Date :&nbsp;&nbsp;@ViewBag.ShowQuoteDate</label>
				</div>
				@*<div style="border: 1px solid black;">*@
				<div style="text-align: right; height: 160px; padding-top: 30px; padding-left: 50px">
					<table>
						<tr>
							<td style="text-align: left; padding-left: 70px">
								<label class="fntst1">
									M/s @ViewBag.OrganizationName
								</label>
							</td>
						</tr>
						<tr>
							<td style="text-align: left; padding-left: 70px">
								<label class="fntst1">@ViewBag.AddressLine1</label>
								@*@ViewBag.AddressLine1*@
							</td>
						</tr>
						<tr>
							<td style="text-align: left; padding-left: 70px">
								<label class="fntst1">@ViewBag.AddressLine2</label>
								@*@Html.DisplayFor(model => model.QGEquipGeneralData.MDBGeneralData.AddressLine2, new { value = @ViewBag.AddressLine2 })*@
							</td>
						</tr>
						<tr>
							<td style="text-align: left; padding-left: 70px">
								<label class="fntst1">@ViewBag.AddressLine3</label>
								@*@Html.DisplayFor(model => model.QGEquipGeneralData.MDBGeneralData.AddressLine3, new { @value = @ViewBag.AddressLine3 })*@
							</td>
						</tr>
						<tr>
							<td style="text-align: left; padding-left: 70px">
								<label class="fntst1">@ViewBag.City</label>
								@*@Html.DisplayFor(model => model.QGEquipGeneralData.MDBGeneralData.City, new { @value = @ViewBag.City })*@
							</td>
						</tr>
						<tr>
							<td style="text-align: left; padding-left: 70px">
								<label class="fntst1">@ViewBag.Pincode</label>
								@*@Html.DisplayFor(model => model.QGEquipGeneralData.MDBGeneralData.Pincode, new { @value = @ViewBag.Pincode })*@
							</td>
						</tr>
						<tr>
							<td style="text-align: left; padding-left: 70px">
								<label class="fntst1">@ViewBag.State</label>
								@*@Html.DisplayFor(model => model.QGEquipGeneralData.MDBGeneralData.State, new { @value = @ViewBag.State })*@
							</td>
						</tr>
					</table>
				</div>
				@* Customer Unique Id , Quotation Number and Channel Partner Unique ID  *@
				<div class="row" style="padding-top: 40px; padding-left: 90px">
					<div class=" row show-grid">
						<div class="span10 pad">
							<div class="span3">
								<div class="control-group">
									<label class="control-label fntst">Customer Unique ID</label>
									<div class="controls">
										@*<input type="text" class="fntst" id="address" placeholder="Customer Unique ID" readonly="readonly">*@
										@Html.TextBoxFor(model => model.OAEquipGeneralData.CompanyUniqueID, new { id = "custuid", Value = @ViewBag.CompanyUniqueID, @class = "fntst", @placeholder = "Customer Unique ID", @readonly = "readonly" })
										@Html.ValidationMessageFor(model => model.OAEquipGeneralData.CompanyUniqueID)
									</div>
								</div>
							</div>
							<div class="span3">
								<div class="control-group">
									<label class="control-label fntst">Quotation Number</label>
									<div class="controls">
										@*<input type="text" class="fntst" id="address" placeholder="Quotation Number" readonly="readonly">*@
										@Html.TextBoxFor(model => model.OAEquipGeneralData.QuotationNumber, new { id = "quotno", Value = @ViewBag.QuotationNumber, @class = "fntst", @placeholder = "Quotation Number", @readonly = "readonly" })
										@Html.ValidationMessageFor(model => model.OAEquipGeneralData.QuotationNumber)
									</div>
								</div>
							</div>
							</div>
							<div class="span12 pad">
							<div class="span3">
								<div class="control-group">
									<label class="control-label fntst">Order Acknowledgement Number</label>
									<div class="controls">
										@*<input type="text" class="fntst" id="address" placeholder="Quotation Number" readonly="readonly">*@
										@Html.TextBoxFor(model => model.OAEquipGeneralData.OANumber, new { id = "quotno", Value = @ViewBag.OANumber, @class = "fntst", @placeholder = "Quotation Number", @readonly = "readonly" })
										@Html.ValidationMessageFor(model => model.OAEquipGeneralData.OANumber)
									</div>
								</div>
							</div>
							<div class="span3">
								<div class="control-group">
									<label class="control-label fntst">Channel Partner Quotation Number</label>
									<div class="controls">
										@*<input type="text" class="fntst" id="address" placeholder="Channel Partner Quotation number">*@
										@Html.TextBoxFor(model => model.OAEquipGeneralData.CPQuotationNumber, new { id = "cpqn", @class = "fntst", @readonly = "readonly", @placeholder = "Channel Partner Quotation number" })
										@Html.ValidationMessageFor(model => model.OAEquipGeneralData.CPQuotationNumber)
									</div>
								</div>
							</div>
							<div class="span3">
								<div class="control-group">
									<label class="control-label fntst">GST Number</label>
									<div class="controls">
										@Html.TextBoxFor(model => model.OAEquipGeneralData.TinNumber, new { id = "tin", Value = @ViewBag.TinNumber, @class = "fntst", @placeholder = "Tin number",@readonly="readonly" })
										@*@Html.ValidationMessageFor(model => model.OAEquipGeneralData.CPQuotationNumber)*@
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
				@* Kind Attention *@
				<div style="padding-left: 120px; height: 40px; padding-top: 10px">
					<table>
						<tr>
							<td>
								<label class="fntst1"><strong>Kind Attention</strong></label></td>
							<td>
								<label class="fntst1">
									<strong>&nbsp;@ViewBag.ContactPersonName,
									</strong>
								</label>
							</td>
						</tr>
					</table>
				</div>
				@* Salutation and Subject *@
				<div style="padding-left: 120px; height: 40px; padding-bottom: 70px">
					<table>
						<tr>
							<td>
								<label class="fntst1">Dear &nbsp;@ViewBag.dear,</label>
							</td>
						</tr>
					</table>
					<table>
						<tr>
							<td>
								<label class="fntst1">Subject :</label></td>
							<td>&nbsp;&nbsp;&nbsp;&nbsp;@*<input type="text" class="input-xxxlarge fntst" id="dearsir" value="">*@
								@Html.TextBoxFor(model => model.OAEquipGeneralData.Subjectinfo, new { id = "sub", @readonly = "readonly", @class = "input-xxxlarge fntst", @required = "required" })
								@Html.ValidationMessageFor(model => model.OAEquipGeneralData.Subjectinfo)
							</td>
						</tr>
					</table>
					<table>
						<tr>
							<td>
								@*<input type="text" class="input-xxxlarge fntst" id="dearsir" value="">*@
								@Html.TextBoxFor(model => model.OAEquipGeneralData.KindAttention, new { id = "salu", @readonly = "readonly", @class = "input-xxxlarge fntst", @required = "required" })
								@Html.ValidationMessageFor(model => model.OAEquipGeneralData.KindAttention)
							</td>
						</tr>
					</table>
				</div>
				<div style="background: gray; height: 2px; width: 1000px"></div>
				<br />
				<div class="row" style="padding-top: 5px; padding-left: 30px">
					<div class=" row show-grid" style="padding-left: 32px">
						<table class="table" style="width: 995px; padding-left: 300px">
